{
	"name": "@magnolia/cli",
	"version": "5.3.0",
	"description": "CLI for light development of Magnolia projects",
	"main": "bin/mgnl.js",
	"bin": {
		"mgnl": "bin/mgnl.js"
	},
	"type": "module",
	"scripts": {
		"prepare": "husky || true",
		"test": "npm run build && node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"build": "tsc && cpy package.json dist && cpy mgnl.config.js dist && cpy resources dist && cpy lib/locales dist",
		"depcheck": "depcheck",
		"lint": "eslint .",
		"lint:fix": "npm run lint -- --fix",
		"format": "prettier \"./**/*.{ts,js,json}\" --ignore-path .prettierignore --ignore-unknown --check",
		"format:write": "npm run format -- --write"
	},
	"lint-staged": {
		"**/*.{ts,js,json}": [
			"npm run format:write",
			"npm run lint:fix"
		]
	},
	"author": "Magnolia International Ltd.",
	"license": "SEE LICENSE IN LICENSE.txt",
	"repository": {
		"type": "git",
		"url": "https://gitlab.magnolia-platform.com/magnolia-cli/npm-cli/-/tree/main"
	},
	"homepage": "https://mgnl.io/cli",
	"devDependencies": {
		"@babel/core": "^7.28.4",
		"@babel/preset-env": "^7.28.3",
		"@babel/preset-typescript": "^7.27.1",
		"@jest/globals": "^29.7.0",
		"@types/fs-extra": "^11.0.4",
		"@types/glob-watcher": "^5.0.5",
		"@types/inquirer": "^9.0.9",
		"@types/js-beautify": "^1.14.3",
		"@types/node": "^24.5.2",
		"@types/uuid": "^11.0.0",
		"babel-plugin-transform-import-meta": "^2.3.3",
		"cpy-cli": "^6.0.0",
		"depcheck": "^1.4.7",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@inquirer/core": "^10.2.2",
		"@magnolia/cli-generate-plugin": "^1.0.3",
		"@magnolia/cli-helper": "^1.1.1",
		"@magnolia/cli-jumpstart-plugin": "^1.1.5",
		"@magnolia/cli-plugin-template": "^1.2.1",
		"axios": "^1.12.2",
		"chalk": "^5.6.2",
		"commander": "^12.1.0",
		"eslint": "^9.36.0",
		"execa": "^9.6.0",
		"fs-extra": "^11.3.2",
		"glob": "^11.0.3",
		"glob-watcher": "^6.0.0",
		"handlebars": "^4.7.8",
		"inquirer": "^12.9.6",
		"js-beautify": "^1.15.4",
		"json5": "^2.2.3",
		"lint-staged": "^16.2.0",
		"moment-timezone": "^0.6.0",
		"ora": "^8.2.0",
		"prettier": "^3.6.2",
		"uuid": "^13.0.0",
		"winston": "^3.17.0"
	}
}
