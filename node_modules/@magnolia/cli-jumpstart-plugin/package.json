{
	"name": "@magnolia/cli-jumpstart-plugin",
	"version": "1.1.5",
	"description": "A plugin for Magnolia CLI to download and set up a new headless or freemarker-based project with Magnolia webapp",
	"main": "dist/index.js",
	"scripts": {
		"prepare": "husky || true",
		"test": "npm run lint && npm run build && node --experimental-vm-modules node_modules/jest/bin/jest.js",
		"build": "tsc && cpy package.json dist && cpy lib/locales dist",
		"depcheck": "depcheck",
		"lint": "eslint .",
		"lint:fix": "npm run lint -- --fix",
		"format": "prettier \"./**/*.{ts,js,json}\" --ignore-path .prettierignore --ignore-unknown --check",
		"format:write": "npm run format -- --write"
	},
	"lint-staged": {
		"**/*.{ts,js,json}": [
			"npm run format:write",
			"npm run lint:fix"
		]
	},
	"keywords": [],
	"author": "Magnolia International Ltd.",
	"license": "SEE LICENSE IN LICENSE.txt",
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@types/decompress": "^4.2.7",
		"@types/filenamify": "^3.0.1",
		"@types/fs-extra": "^11.0.4",
		"@types/inquirer": "^9.0.9",
		"@types/js-beautify": "^1.14.3",
		"@types/node": "^24.5.2",
		"@types/progress": "^2.0.7",
		"@types/underscore": "^1.13.0",
		"@types/walk": "^2.3.4",
		"cpy-cli": "^6.0.0",
		"depcheck": "^1.4.7",
		"eslint": "^9.36.0",
		"husky": "^9.1.7",
		"jest": "^29.7.0",
		"lint-staged": "^16.2.0",
		"prettier": "^3.6.2",
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@babel/generator": "^7.28.3",
		"@babel/parser": "^7.28.4",
		"@babel/traverse": "^7.28.4",
		"@babel/types": "^7.28.4",
		"@magnolia/cli-helper": "^1.1.1",
		"@magnolia/cli-plugin-template": "^1.2.1",
		"axios": "^1.12.2",
		"chalk": "^5.6.2",
		"commander": "^12.1.0",
		"decompress": "^4.2.1",
		"dotenv": "^17.2.2",
		"execa": "^9.6.0",
		"filenamify": "^7.0.0",
		"fs-extra": "^11.3.2",
		"glob": "^11.0.3",
		"http-proxy-agent": "^7.0.2",
		"https-proxy-agent": "^7.0.6",
		"inquirer": "^12.9.6",
		"js-beautify": "^1.15.4",
		"json5": "^2.2.3",
		"ora": "^9.0.0",
		"progress": "^2.0.3",
		"underscore": "^1.13.7",
		"walk": "^2.3.15",
		"winston": "^3.17.0",
		"yaml": "^2.8.1"
	},
	"type": "module",
	"standard": {
		"ignore": [
			"tests/resources/config-helper/*/mgnl.config.js"
		]
	}
}
